@{
    ViewData["Title"] = "Home Page";
}

<div class="row">
        <div class="col-md-6 offset-md-3 jumbotron">
            <form method="post" action="/home/upload" enctype="multipart/form-data">
                <input type="file" name="image" placeholder="Select Image" class="form-control" />
                <br />
                <input type="password" class="form-control" name="password" placeholder="Custom Password" />
                <br />
                <button class="btn btn-primary">Upload</button>
            </form>
        </div>
    </div>

